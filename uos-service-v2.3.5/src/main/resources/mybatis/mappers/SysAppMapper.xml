<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.imapcloud.nest.mapper.SysAppMapper">

    <select id="getInfoById" resultType="com.imapcloud.nest.model.SysAppEntity">
         select
            sa.*
        from sys_app sa
        where sa.id = #{appId}
    </select>

</mapper>
